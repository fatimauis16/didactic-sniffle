a=int(input('enter the number'))
num1=0
num2=1
count=0
if a<=0:
    print('enter positive number')
elif a==1:
    print(a)
    print(num1)
else:
    while count<a:  
        print(num1)
        num=num1+num2
        num1=num2
        num2=num
        count+=1
-----------------------------------------
num=int(input('enter starting integer'))
num1=int(input('enter ending integer'))
for i in range(num,num1):
    if i%2==0:
        print(i)
